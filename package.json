{
  "name": "manabie-todo-challenge",
  "description": "Manabie todo challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "antd": "^4.19.3",
    "axios": "^0.19.2",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "4.0.3",
    "shortid": "^2.2.15",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start:fullstack": "cross-env REACT_APP_WHOAMI=fullstack react-scripts start",
    "start:frontend": "concurrently \"yarn css:dev\" \"cross-env REACT_APP_WHOAMI=frontend react-scripts start\"",
    "css:dev": "tailwindcss -i ./src/base.css -o ./src/styles/styles.css --watch",
    "css": "tailwindcss -i ./src/base.css -o ./src/styles/styles.css --minify",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/shortid": "^0.0.29",
    "concurrently": "^7.0.0",
    "tailwindcss": "^3.0.23"
  }
}
